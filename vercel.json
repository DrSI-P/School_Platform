{
  "version": 2,
  "public": false,
  "github": {
    "enabled": true,
    "silent": true
  },
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/usage-analytics",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/subscription-reminders",
      "schedule": "0 12 * * *"
    }
  ]
}